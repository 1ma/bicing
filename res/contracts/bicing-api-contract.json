{
  "$schema": "http://json-schema.org/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "pattern": "^\\d{1,3}$"
      },
      "lat": {
        "type": "string",
        "pattern": "^\\d{1,3}\\.\\d{2,6}$"
      },
      "lon": {
        "type": "string",
        "pattern": "^\\d{1,3}\\.\\d{2,6}$"
      },
      "address": {
        "type": "string"
      },
      "addressNumber": {
        "type": "string"
      },
      "bikes": {
        "type": "string",
        "pattern": "^\\d{1,2}$"
      },
      "slots": {
        "type": "string",
        "pattern": "^\\d{1,2}$"
      },
      "status": {
        "type": "string",
        "pattern": "^OPN|CLS$"
      },
      "stationType": {
        "type": "string",
        "pattern": "^(?:ELECTRIC_)?BIKE$"
      }
    },
    "required": [
      "id",
      "district",
      "lon",
      "lat",
      "bikes",
      "slots",
      "zip",
      "address",
      "addressNumber",
      "nearbyStations",
      "status",
      "name",
      "stationType"
    ]
  }
}
